{
  "name": "shopsense-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python app.py",
    "dev": "npx concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "install:all": "cd frontend && npm install",
    "setup:backend": "cd backend && pip install -r requirements.txt"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
